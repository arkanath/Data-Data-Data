<script type="text/javascript">
    zmCore.ready(function(){
        TVSlider.init();
        Charts.init();
    });
    var Charts = {
        init:function(){
            zm('._charts_filter').click(function(){
                var genreId = zm(this).attr("genreId");
                var genreName = zm(this).html();
                zm('#_charts_title').html(genreName);
                zm('#_genreCatBox').addClass("none");
                //alert(genreId);
                Charts.loadCharts(genreId);
                return false;
            });
			//gom 8 phan tu
			var genreIds=new Array("IWZ9Z0CE","IWZ9Z0DC","IWZ9Z0DW","IWZ9Z0DO","IWZ9Z0D7","IWZ9Z0DI","IWZ9Z0D6","IWZ9Z0CF");
			var genreNames=new Array("TV Show","Âm Nhạc","Phim Truyền Hình","Hoạt Hình","Giáo Dục","Thể Thao","Hài Hước","Tin Tức - Sự Kiện");
			var randomIndex = Math.floor((Math.random()*7)+1);
            //var defaultGenreId = "IWZ9Z0CE";//tvshow
			var defaultGenreId = genreIds[randomIndex];
			var defaultGenreName = genreNames[randomIndex];
			zm('#_charts_title').html(defaultGenreName);
            Charts.loadCharts(defaultGenreId);
        },
        hideOtherChartsOfGenre: function(genreId){
            var allChart = zm('#_home_charts_items').children("div");
            for(var i = 0; i<allChart.length;i++){
                if(allChart[i].id != '_home_charts_items_'+genreId){
                    allChart[i].style.display="none";
                }else{
                    allChart[i].style.display="block";
                }
            }
            zm('._trackLinkCharts').click(function(){
                var tar = zm(this);
                if(tar.attr('tagName')!='A'){
                    tar = tar.parent('a');
                }
                var href = tar.attr('href');
                if(href.search(MP3.TV_URL)!=-1) href='/'+href.replace(MP3.TV_URL,'');
                if(href.search("http://")!=-1) href='/'+href.replace("http://",'');

                _gaq.push(['tv._trackPageview', '/tracking'+href + '?'+ tar.attr('tracking')]);
                if (location.href.indexOf('debug') > -1) {
                    Common.alert('/tracking'+href + '?'+ tar.attr('tracking'));
                    return false;
                }
            });
        },
        loadCharts: function(genreId){   
            if(zm('#_home_charts').size()){
                if(zm("#_home_charts_items_"+genreId).size()){
                    Charts.hideOtherChartsOfGenre(genreId);
                }else{
                    zmCore.get(
                    MP3.TV_URL + "tv/ajaxtv/suggest/get-charts"+"?genreId="+genreId,
                    function(obj) {
                        if(obj){
                            zm('#_home_charts_items').append(obj);
                            Charts.hideOtherChartsOfGenre(genreId);
                        }

                    });
                }
                
            }
        }
    };
